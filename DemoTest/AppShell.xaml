<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DemoTest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DemoTest"
    xmlns:pages="clr-namespace:DemoTest.Pages"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="DefaultTestPage"
        ContentTemplate="{DataTemplate pages:DefaultTestPage}"
        Route="DefaultTestPage" />
    <ShellContent
        Title="DefaultScrollViewTestPage"
        ContentTemplate="{DataTemplate pages:DefaultScrollViewTestPage}"
        Route="DefaultScrollViewTestPage" />
    <ShellContent
        Title="CollectionViewTestPage"
        ContentTemplate="{DataTemplate pages:CollectionViewTestPage}"
        Route="CollectionViewTestPage" />

</Shell>
